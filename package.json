{
  "name": "jquery-spaghetti",
  "version": "0.0.1",
  "description": "modular jquery with handlebars.js and bootstrap",
  "main": "index.js",
  "scripts": {
    "prestart": "npm i",
    "postinstall": "npm prune",
    "stop": "npm run gradle -- --stop",
    "sleep": "sleep 7 # netstat -plten |grep node",
    "browser-sync": "browser-sync --config=./config/bs-config.js start",
    "gradle": "bash ./jquery-spaghetti-backend/gradlew -b ./jquery-spaghetti-backend/build.gradle ",
    "start:frontend": "npm-run-all sleep browser-sync",
    "start:backend": "npm run gradle -- -a bootRun",
    "start": "npm-run-all -p start:*"
  },
  "author": "daggerok",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "handlebars": "^4.0.5",
    "jquery": "^3.0.0"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "http-proxy-middleware": "^0.16.0",
    "npm-run-all": "^2.2.0"
  },
  "keywords": [
    "jquery",
    "bootstrap",
    "handlebars",
    "browser-sync",
    "http-proxy-middleware"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/jquery-spaghetti.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/jquery-spaghetti/issues"
  },
  "homepage": "https://github.com/daggerok/jquery-spaghetti#readme"
}
